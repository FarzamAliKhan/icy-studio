    background-clip: text;
    filter: drop-shadow(4px 9px 4px rgba(0, 0, 0, 1));
}

.gradient-customers {
    background: linear-gradient(1.69deg, #B3E823 -14.96%, #FAD4D4 49.79%, #FFFFFF 98.64%);
    background-clip: text;
}

.gradient-text1 {
    background: linear-gradient(360deg, #999999 -41.04%, #FFFFFF 77.83%);
    background-clip: text;
    filter: drop-shadow(-8px 7px 4px rgba(0, 0, 0, 0.8));
}

.box-shadow2{
    text-shadow: 2px 6px 4px rgba(0, 0, 0, 1);
}

.box-shadow3{
    text-shadow: 2px 8px 5.6px rgba(0, 0, 0, 1);
}

.box-shadow4{
    text-shadow: 5px 9px 5.2px rgba(0, 0, 0, 1);
}

.box-shadow5{
    text-shadow: 4px 4px 4px rgba(0, 0, 0, 1);
}

.box-shadow6{
    filter: drop-shadow(-6px 7px 4px rgba(0, 0, 0, 0.96));
}


</style>

<script>
    const degreeToRadian = (angle: number): number => {
        return angle * (Math.PI / 180);
        };

    const radius: number = 80;
    const diameter: number = radius * 2;

    const circle: HTMLElement | null = document.querySelector("#circle");
    if (circle) {
    circle.style.width = `${diameter}px`;
    circle.style.height = `${diameter}px`;

        const text: string | undefined = circle.dataset.text;
        if (text) {
            const characters: string[] = text.split("");

            const deltaAngle: number = 360 / characters.length;
            const characterOffsetAngle: number = 8;
            let currentAngle: number = -90;

            characters.forEach((character, index) => {
            const span: HTMLSpanElement = document.createElement("span");
            span.innerText = character;
            const xPos: number = radius * (1 + Math.cos(degreeToRadian(currentAngle)));
            const yPos: number = radius * (1 + Math.sin(degreeToRadian(currentAngle)));

            const transform: string = `translate(${xPos}px, ${yPos}px)`;
            const rotate: string = `rotate(${(index * deltaAngle) + characterOffsetAngle}deg)`;
            span.style.transform = `${transform} ${rotate}`;
            span.style.position = "absolute";
            span.style.transformOrigin = "top left";


            currentAngle += deltaAngle;
            if (circle) circle.appendChild(span);
            });
        }
    }
</script>