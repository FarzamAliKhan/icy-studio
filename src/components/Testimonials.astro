<style >
     .gradient {
        background: linear-gradient(97.33deg, var(--neon-green) -24.15%, #FCE1E1 44.59%, #FFFFFF 96.44%);
        background-clip: text;
    }

    .outlined-text {
        color: #000000; /* Text color */
        text-shadow: 
            -1px -1px 0 var(--neon-green) ,  
             1px -1px 0 var(--neon-green) ,
            -1px  1px 0 var(--neon-green) ,
             1px  1px 0 var(--neon-green) ; /* Shadow positions and colors */
    }

    .outlined-text-red {
        color: #000000; /* Text color */
        text-shadow: 
            -1px -1px 0 var(--blush-red) ,  
             1px -1px 0 var(--blush-red) ,
            -1px  1px 0 var(--blush-red) ,
             1px  1px 0 var(--blush-red) ; /* Shadow positions and colors */
    }

    .outlined-text-black {
        color: var(--neon-green); /* Text color */
        text-shadow: 
            -5px -5px 0 var(--background) ,  
             5px -5px 0 var(--background) ,
            -5px  5px 0 var(--background) ,
             5px  5px 0 var(--background) ; /* Shadow positions and colors */
    }

    .gradient-follow-us{
        background: linear-gradient(323.6deg, #B3E823 11.51%, #FFDEDE 71.47%, #FFFFFF 120.52%);
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
        background-clip: text;
        text-fill-color: transparent;
    }

    /* box-shadow testimonial boxes */
    
    .box-shadow-testimonial{
        box-shadow: inset 0px 0px 4px 5px #000000;
    }


</style>

